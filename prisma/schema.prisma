
generator client {
  provider = "prisma-client-js"
}

datasource db {
provider = "postgresql"
url = env("DATABASE_URL")
}

model register_use {
    id_user        Int      @id @default(autoincrement())
    nome_completo  String
    email          String
    cpf            String
    genero         String
    data_nascimento String
    senha          String
    senha_confirm  String

    @@map("register_use")
}

